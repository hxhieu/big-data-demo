@model HDInsight.Models.Account.CreateAccountModel

<h1>Create new account</h1>
<form asp-controller="Account" asp-action="Index" method="post">
    <table>
        <tr>
            <td>Email</td>
            <td>@Html.TextBoxFor(x => x.Email)</td>
        </tr>
        <tr>
            <td>Password</td>
            <td>@Html.PasswordFor(x => x.Password)</td>
        </tr>
    </table>
    <button type="submit">Create</button>
</form>